{
  "name": "postfy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-openapi-gen": "ng-openapi-gen",
    "generate.network": "ng-openapi-gen --input http://localhost:9000/swagger/v1/swagger.json -o src/app/api/network --module NetworkApiModule --skipJsonSuffix true --removeStaleFiles false",
    "generate.identity": "ng-openapi-gen --input http://localhost:7000/swagger/v1/swagger.json -o src/app/api/identity --module IdentityApiModule --skipJsonSuffix true --removeStaleFiles false",
    "generate": "npm run generate.network && npm run generate.identity"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@microsoft/signalr": "^7.0.10",
    "@types/google-one-tap": "^1.2.2",
    "angular-animations": "^0.11.0",
    "angular-svg-icon": "^13.0.0",
    "date-fns": "^2.30.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.14",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.8.2"
  }
}
